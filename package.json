{
  "author": "paul@savignano.io",
  "bin": {
    "mtg": "bin/genTypes.js",
    "mtg-gen-types": "bin/genTypes.js",
    "mtg-download-validators": "bin/downloadValidators.js",
    "mtg-upload-validators": "bin/uploadValidators.js"
  },
  "bugs": {
    "url": "https://github.com/PaulSavignano/mongo-type-gen/issues"
  },
  "dependencies": {
    "fast-glob": "^3.3.0",
    "fs": "^0.0.1-security",
    "glob": "^10.3.3",
    "graphql-tag": "^2.12.6",
    "json-schema": "^0.4.0",
    "mongodb": "^5.7.0",
    "typescript": "^5.1.6"
  },
  "description": "A type and sdl generator for MongoDB",
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "esbuild": "^0.18.14",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sort-class-members": "^1.18.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1"
  },
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "files": [
    "bin",
    "LICENSE"
  ],
  "homepage": "https://github.com/PaulSavignano/mongo-type-gen#readme",
  "keywords": [
    "GraphQL",
    "Mongo",
    "Typescript"
  ],
  "license": "ISC",
  "main": "mongo-type-gen.js",
  "name": "mongo-type-gen",
  "engines": {
    "node": ">=18.13.0"
  },
  "peerDependencies": {
    "graphql-tag": "^2.0.0",
    "mongodb": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulSavignano/mongo-type-gen.git"
  },
  "scripts": {
    "upload-validators": "ts-node ./src/uploadValidators.ts",
    "build": "rm -rf bin && node esbuild.mjs",
    "checks:ci": "npm run format && npm run lint && npm run typecheck && npm run build",
    "docker:down": "docker-compose -f docker-compose.local.yml down --remove-orphans",
    "docker:up": "docker-compose -f docker-compose.local.yml up -d",
    "format": "prettier --list-different '**/*.{css,js,jsx,ts,tsx,json,htm,html,yml,yaml}'",
    "format:fix": "prettier --write '**/*.{css,js,jsx,ts,tsx,json,htm,html,yml,yaml}'",
    "gen-types": "ts-node ./src/genTypes.ts -w",
    "gen-types:build": "node ./bin/genTypes.js",
    "download-validators": "ts-node ./src/downloadValidators.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "testing": "ts-node-dev ./src/testing.ts",
    "type-gen:build": "node ./bin/type-gen.js -w",
    "typecheck": "tsc --project . --noEmit"
  },
  "version": "1.0.24"
}